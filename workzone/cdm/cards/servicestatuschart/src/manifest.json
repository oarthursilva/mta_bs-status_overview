{
  "_version": "1.14.0",
  "sap.app": {
    "id": "cards.servicestatuschart",
    "type": "card",
    "title": "{{title}}",
    "subTitle": "{{subtitle}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "description": "{{description}}",
    "tags": {
      "keywords": [
        "Analytical",
        "Card",
        "Donut"
      ]
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "phone": true,
      "tablet": true,
      "desktop": true
    },
    "icons": {
      "icon": "sap-icon://donut-chart"
    }
  },
  "sap.card": {
    "type": "Analytical",
    "header": {
      "type": "Numeric",
      "data": {
        "json": {
          "number": "2",
          "unit": "errors",
          "state": "Error",
          "details": "Q3, 2025"
        }
      },
      "title": "{{title}}",
      "subtitle": "{{subtitle}}",
      "unitOfMeasurement": "",
      "mainIndicator": {
        "number": "{number}",
        "unit": "{unit}",
        "trend": "{trend}",
        "state": "{state}"
      },
      "details": "{details}",
      "sideIndicators": []
    },
    "content": {
      "chartType": "Donut",
      "chartProperties": {
        "title": {
          "text": "{{contentChartPropertiesTitleText}}",
          "visible": true,
          "alignment": "left"
        },
        "legend": {
          "visible": "{legend/visible}"
        },
        "legendGroup": {
          "layout": {
            "position": "{legend/position}",
            "alignment": "{legend/alignment}"
          }
        },
        "plotArea": {
          "dataLabel": {
            "visible": true
          }
        },
        "categoryAxis": {
          "title": {
            "visible": false
          }
        },
        "valueAxis": {
          "title": {
            "visible": false
          }
        }
      },
			"data": {
				"json": [
						{
							"Status": "Success",
							"Count": 4
						},
						{
							"Status": "Error",
							"Count": 2
						}
					]
			},
			"dimensions": [
				{
					"name": "Status",
					"value": "{Status}"
				}
			],
			"measures": [
				{
					"name": "Count",
					"value": "{Count}"
				}
			],
			"feeds": [
				{
					"type": "Dimension",
					"uid": "color",
					"values": [
						"Status"
					]
				},
				{
					"type": "Measure",
					"uid": "size",
					"values": [
						"Count"
					]
				}
			]
    }
  }
}